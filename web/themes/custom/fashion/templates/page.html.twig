
<div class="container">
  <span class="page-overlay" style="display: none;"></span>
  {% if page.header  %}
    <header class="header">
      <div class="wrap">
    {{ page.header }}
      {% if page.below_header  %}
        <div class="header-search">
          {{ page.below_header }}
        </div>
      {% endif %}
      </div>
    </header>
  {% endif %}

  {% if page.content  %}
    <div class="content">
      {{ page.content }}
    </div>
  {% endif %}


  <footer class="footer">
    {% if page.above_footer  %}
      {#<div class="services">#}
        {{ page.above_footer }}
      {#</div>#}
    {% endif %}
    {{ page.footer }}
  </footer>

</div>



{#<div id="page-wrapper">#}
